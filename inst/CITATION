year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{metajags}: R package to for specifying JAGS models as R code",
         author = c(person("Matthew", "Kay")),
         year = year,
         note = note,
         url = "https://github.com/mjskay/metajags")
         